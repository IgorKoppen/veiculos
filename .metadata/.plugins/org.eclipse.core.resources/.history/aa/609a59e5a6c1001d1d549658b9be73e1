package br.com.fiap;

import java.util.Date;

public final class Carro extends Veiculo {



	public Carro(String nomeModelo, String marca, String cor, String embreagem, String tipoCombustivel,
			Date anoFabricacao, int velocidade, int maxVelocidade, double combustivel, double peso,
			boolean estaLigado) {
		super(nomeModelo, marca, cor, embreagem, tipoCombustivel, anoFabricacao, velocidade, maxVelocidade, combustivel, peso,
				estaLigado);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void ShowVeiculosStatus() {
		System.out.println("Modelo:" + this.nomeModelo);
		System.out.println("Marca:" + this.marca);
		System.out.println("Cor:" + this.cor);
		System.out.println("Embreagem:" + this.embreagem);
		System.out.println("Ano de fabriaçao:" + this.anoFabricacao);
		System.out.println("tipo Combustivel:" + this.combustivel);
	}



}
